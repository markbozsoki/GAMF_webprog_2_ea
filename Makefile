php_version:
	\php\php -version

artisan:
	\php\php artisan

artisan_version:
	\php\php artisan --version

artisan_serve:
	\php\php artisan serve

artisan_route_list:
	\php\php artisan route:list

artisan_make_migration:
	\php\php artisan make:migration migration_file

artisan_migrate:
	\php\php artisan migrate

artisan_migrate_status:
	\php\php artisan migrate:status

artisan_migrate_refresh:
	\php\php artisan migrate:refresh

artisan_migrate_fresh:
	\php\php artisan migrate:fresh

artisan_make_seeder:
	\php\php artisan make:seeder Seeder

artisan_db_seed:
	\php\php artisan db:seed

artisan_migrate_fresh_seed:
	\php\php artisan migrate:fresh --seed
